.content .rankings .list {
  display: flex;
  flex-direction: row;
  margin: 20rpx;
  background: #fff;
}

.content .rankings .list .left {
  width: 200rpx;
  height: 200rpx;
  position: relative;
}

.content .rankings .list .left >image {
  width: 200rpx;
  height: 200rpx;
}

.content .rankings .list .right {
  display: flex;
  flex-direction: column;
  font-size: 28rpx;
  padding: 10rpx 10rpx 10rpx 30rpx;
  box-sizing: border-box;
  font-family: "arial";
  color: rgba(0, 0, 0, 0.5);
  overflow: hidden;
  flex: auto;
}

.content .rankings .list .right .cate {
  font-size: 32rpx;
  margin: 10rpx 0 14rpx 0;
  color: #000;
}

.content .rankings .list .right .name {
  color: #000;
  margin: 0 16rpx;
}

.content .rankings .list .right .txt {
  line-height: 40rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.content .rankings .list .go {
  position: relative;
  padding: 0 10rpx;
  width: 10px;
}

.content .rankings .list .go .arrow {
  width: 6px;
  height: 6px;
  border-right: 1px solid #b2b2b2;
  border-bottom: 1px solid #b2b2b2;
  position: absolute;
  top: 50%;
  margin-top: -3px;
  transform: rotate(-45deg);
}

.content .list .left .middle {
  opacity: 0.6;
  bottom: 10rpx;
  left: 2rpx;
}
